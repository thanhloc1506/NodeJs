<h1>Weather app</h1>
<h2>{{location}}</h2>

<ul style="list-style-type: none;">
{{#each weather_date}}
    <li style="background-color: beige; border-radius: 2px; padding-left: 20px;">{{@key}}: {{this}}</li>
{{/each}}
<img src="https://www.metaweather.com//static/img/weather/{{weather_date.weather_state_abbr}}.svg" alt="Icon" width="80" height="80">
</ul>