<h1>Weather app</h1>
<h2>{{location}}</h2>
{{#if weather }}
<ul style="list-style-type: none;">
    {{#each weather}}
    {{#each this}}
    <li style="background-color: beige; border-radius: 2px; padding-left: 20px;">{{@key}}: {{this}}</li>
    {{/each}}
    </br>
    {{/each}}
</ul>
{{/if}}
